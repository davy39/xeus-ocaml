{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2022",
      "dom",
      "dom.iterable"
    ],
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "target": "es2022",
    "types": [],
        "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": ["src/*"]
}
